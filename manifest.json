{
  "name" : "Identity Switcher",
  "version" : "0.1",
  "description" : "Identity Switcher allows for local recording of open tabs, bookmarks, and cookies per user's identity",
  "permissions": [
    "storage",
    "cookies",
    "bookmarks",
    "tabs",
    "http://*/*", "https://*/*"
  ],
  "icons": { "16": "cookie.png", "48": "cookie.png", "128": "cookie.png" },
   "browser_action": {
      "default_icon": "cookie.png",
      "default_title": "Identity Switcher",
      "default_popup": "popup.html"
   },
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2
}
